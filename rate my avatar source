Tab:AddButton({
	Name = "Chat Logger Booth",
	Callback = function()
      		local players = game.Players:GetChildren()
local player = game.Players.LocalPlayer

for i, player in pairs(players) do
    if player == player then
        player.Chatted:Connect(function(message)
            local args = {
                [1] = "Update",
                [2] = {
                 ["DescriptionText"] = (player.Name .. ": " .. message),
                 ["ImageId"] = 6429025023
             }
            }

game:GetService("ReplicatedStorage").CustomiseBooth:FireServer(unpack(args))

        end)
    end
end

  	end    
})

--tab 2
Tab2:AddButton({
	Name = "Chat Logs Sign",
	Callback = function()
      		local players = game.Players:GetChildren()
local player = game.Players.LocalPlayer

for i, player in pairs(players) do
    if player == player then
        player.Chatted:Connect(function(message)
            local args = {
    [1] = "Text",
    [2] = (player.Name .. ": " .. message)
}

game:GetService("ReplicatedStorage").UpdateSign:FireServer(unpack(args))

        end)
    end
end
  	end    
})

-pov trying to reach more words in essay
local bozo = Window:MakeTab({
	Name = "Other",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

bozo:AddSlider({
	Name = "Walkspeed",
	Min = 0,
	Max = 200,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed",
	Callback = function(Value)
		local player = game.Players.LocalPlayer
local character = player.Character
local humanoid = character:WaitForChild("Humanoid")
humanoid.WalkSpeed = (Value)
		
	end    
})

OrionLib:Init()
